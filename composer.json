        "illuminate/console": "5.0.x|5.1.x|5.2.x",
        "illuminate/support": "5.0.x|5.1.x|5.2.x",
        "illuminate/cache": "5.0.x|5.1.x|5.2.x"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.1",
